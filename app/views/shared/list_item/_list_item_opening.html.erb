<li class='list-group-item'>
	<%= link_to user_opening_path(e) do %>
		<div class='row'>
			<div class='col-xs-12 col-sm-6 col-md-6'>
				<span class='profile_color profile_color_<%= e.profile.title.gsub(/\s+/,"")%>'></span>
				<div class='small_profile_title'>
					<p><%= e.profile.title %></p>
					<em class='text-success'>Available</em>
				</div>
			</div>
			
		</div>
		<div class="row">
			<div class='col-xs-6 col-xs-offset-6'>
				<%= e.start.strftime('%I:%M')%> - <%= e.end.strftime('%I:%M %P') %>
			</div>
		</div>
	<% end %>
</li>